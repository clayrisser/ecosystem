    "@typescript-eslint/parser": "^2.3.1",
    "babel-preset-jam": "^0.4.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-typescript": "^5.0.0",
    "eslint-config-jam": "^0.2.20",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.6.3"
  },
