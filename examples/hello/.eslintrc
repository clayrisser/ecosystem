{
  "extends": ["jam/typescript"],
  "rules": {
    "import/export": "off",
    "import/named": "off",
    "import/no-unresolved": "off",
    "no-unused-vars": "off",
    "react/forbid-foreign-prop-types": "off",
    "react/jsx-wrap-multilines": "off",
    "react/no-unescaped-entities": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_" }
    ]
  }
}
